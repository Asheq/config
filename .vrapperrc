" vrapper bonus mappings:
" gR    rename element
" gc    toggle line-comment
" gr    find all references
set ignorecase
set smartcase
set incsearch
set saney
set tabstop=4
set autochdir
nnoremap K i<cr><esc>k$
inoremap jj <esc>
nnoremap ,bd :bdelete<cr>
nnoremap ,w  :w<cr>
nnoremap ,v  :vim<cr>
vnoremap 0 $
vnoremap 1 ^
nnoremap 0 $
nnoremap 1 ^
nnoremap <m-l> :ls<cr>
set gvimpath=/Users/justin/bin/mvim

